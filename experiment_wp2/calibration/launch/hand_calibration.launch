<!-- calculate the transfomation between /bracelet  and hand -->
<launch>
 

<include file="$(find phase_space)/launch/track_object.launch">
  	<arg name="object" value="bracelet" />
	<arg name="turn_rviz_on" value="True" />
</include>

<include file="$(find phase_space)/launch/track_object.launch">
    <arg name="object" value="hand" />
    <arg name="turn_rviz_on" value="True" />
    <arg name="turn_phase_space_on" value="False"/>
</include>

<node pkg="calibration" type="calibration" name="hand_calibration_node" output="screen">
	<param name="parent_frame_name_" value= "/bracelet" />
  	<param name="child_frame_name_" value= "/hand" />
   	<param name="phase_frame_name_" value= "/phase_space_world" />
   	<param name="type_frame_name_" value= "/hand" />
    <param name="calibration_type_" value="hand"/>
   	<param name="tf_launch_file_dir" value="$(find calibration)/launch" />
</node>

	
</launch>



