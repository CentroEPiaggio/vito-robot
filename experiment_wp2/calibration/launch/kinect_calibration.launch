<!-- calculate the transfomation from /camera_rgb_optical_frame to /phase_space_world-->
<launch>
    
	<include file="$(find phase_space)/launch/track_object.launch">
  		<arg name="object" value="bracelet" />
		  <arg name="turn_rviz_on" value="False" />
	</include>

	<node pkg="calibration" type="calibration" name="kinect_calibration_node" output="screen">
  		<param name="manualTxyz" value= "-0.02,0.09, 1.13" />
  		<param name="manualQwxyz" value= "0.31,0.58,0.63,-0.40" />
      <param name="parent_frame_name_" value= "/camera_rgb_optical_frame" />
      <param name="child_frame_name_" value= "/phase_space_world" />
      <param name="phase_frame_name_" value= "/phase_space_world" />
      <param name="type_frame_name_" value="/bracelet"/>
      <param name="calibration_type_" value="camera"/>
      <param name="tf_launch_file_dir" value="$(find calibration)/launch/"/>
  </node>
      

</launch>

