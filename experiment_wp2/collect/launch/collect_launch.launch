<launch>

<arg name="thimbles" default="False" />
<arg name="Nothimbles" default="True" />
<arg name="NoPointCloud" default="True" />

<!-- Static tranformations -->
<include file="$(find calibration)/launch/tf_camera_world.launch"/>
<include file="$(find calibration)/launch/tf_star_object.launch"/>
<include file="$(find calibration)/launch/tf_wrist_hand.launch"/>

<!-- Camera -->
<include file="$(find openni_launch)/launch/openni.launch" /> 

<!-- Dynamic transformations -->

<include file="$(find phase_space)/launch/track_object.launch">
  	<arg name="object" value="star" />
	<arg name="turn_rviz_on" value="False" /> 
	<arg name="turn_phase_space_on" value="True"/>
</include>

<include file="$(find phase_space)/launch/track_object.launch">
  		<arg name="object" value="bracelet" />
		<arg name="turn_phase_space_on" value="False"/>
		<arg name="turn_rviz_on" value="False" />
</include>


<!-- collect also thimble information  
  <group ns="Thimbles" if="$(arg thimbles)">  -->
	
	<include file="$(find phase_space)/launch/track_object.launch">
  		<arg name="object" value="thimble" />
		<arg name="turn_rviz_on" value="False" />
		<arg name="turn_phase_space_on" value="False"/>
	</include>

	 <include file="$(find intrinsic_tactile_toolbox)/launch/thimble_tactile.launch" />  

	<node pkg="collect" name="collectData_thimble_node" type="collectData_node_oneThimble" respawn="true" output="screen"/>
 <!--
</group> 


 collect only the positions of the leds and the pointcloud 
<group ns="NoThimbles" if="$(arg Nothimbles)">
	
	<node pkg="collect" name="collectData_node" type="collectData_node" respawn="true" output="screen"/>
    
</group>


<group ns="NoThimbles" if="$(arg NoPointCloud)">
	
	<node pkg="collect" name="collectData_node" type="collectData_node_nocamera" respawn="true" output="screen"/>
	
    
</group>

-->


 </launch>

     
     
