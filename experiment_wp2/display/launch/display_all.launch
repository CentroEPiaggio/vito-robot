<launch>

<include file="$(find calibration)/launch/tf_camera_world.launch"/>
<include file="$(find calibration)/launch/tf_star_object.launch"/>
<include file="$(find calibration)/launch/tf_wrist_hand.launch"/> 


<include file="$(find phase_space)/launch/track_object.launch">
      <arg name="object" value="bracelet" />
</include> 


<include file="$(find phase_space)/launch/track_object.launch">
      <arg name="object" value="star" />
      <arg name="turn_rviz_on" value="False"/>
     <arg name="turn_phase_space_on" value="False"/>   
</include>


<group ns="object">
    <arg name="model" default="$(find display)/urdf/object.urdf.xacro"/>
    <arg name="child" value="object"/>
    <arg name="parent" value="star"/>
    <arg name="filename" value="cyl_w_closed_handle_in_meters.stl" /> 
    <arg name="scalex" value="1"/>
    <arg name="scaley" value="1"/>
    <arg name="scalez" value="1"/>
    <param name="robot_description" command="$(find xacro)/xacro.py $(arg model) filename:=$(arg filename) child:=$(arg child) parent:=$(arg parent) scalex:=$(arg scalex)  scaley:=$(arg scaley)  scalez:=$(arg scalez)" />
</group> 


